{
  "name": "rusty_reader",
  "version": "1.0.0",
  "description": "RSS feed reader",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css": "node-sass --include-path scss assets/scss/style.scss assets/style.css",
    "cargo": "cargo run",
    "watch": "nodemon -e scss -x 'npm run css'",
    "dev": "npm run watch & npm run cargo"
  },
